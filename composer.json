{
    "name": "aternos/php-rados-ffi",
    "description": "An object oriented PHP library for using librados with FFI",
    "type": "library",
    "bin": ["bin/generate-headers"],
    "require": {
        "ext-ffi": "*"
    },
    "suggest": {
        "ext-posix": "Resolve error codes to strings"
    },
    "license": "LGPL-2.1-only",
    "autoload": {
        "psr-4": {
            "Aternos\\Rados\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Kurt Thiemann",
            "email": "kurt@aternos.org"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    }
}
